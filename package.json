    "typeorm": "typeorm-ts-node-esm -d ./src/db/config/data-source.ts",
    "typeorm:test": "typeorm-ts-node-esm -d ./src/tests/config/test-data-source.ts",
    "typeorm:test:migrations:run": "npm run typeorm:test -- migration:run",
    "start": "npm run build && node ./dist/index.js",
    "build": "tsc",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-mocha src/tests/**/*.spec.ts --exit"
  },
